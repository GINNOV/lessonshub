{
  "name": "lessenhub-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NEXT_PUBLIC_VERCEL_GIT_COMMIT_SHA=$(git rev-parse --short HEAD 2>/dev/null || echo dev) next dev",
    "build": "dotenv -e .env.local -- prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npm run test:build && node --test tmp/test-dist/__tests__/assignmentNotifications.test.js",
    "test:build": "tsc -p tsconfig.test.json",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:push": "dotenv -e .env.local -- prisma db push",
    "prisma:studio": "dotenv -e .env.local -- prisma studio",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:resolve": "dotenv -e .env.local -- prisma migrate resolve",
    "prisma:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "prisma:db:pull": "dotenv -e .env.local -- prisma db pull",
    "prisma:db:push": "dotenv -e .env.local -- prisma db push",
    "prisma:seed": "PRISMA_CLIENT_ENGINE_TYPE=binary dotenv -e .env.local -- tsx prisma/seed.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.1",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/components": "^0.5.1",
    "@react-email/render": "^1.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^0.23.4",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.400.0",
    "marked": "^16.2.1",
    "nanoid": "^5.1.5",
    "next": "15.5.4",
    "next-auth": "^5.0.0-beta.19",
    "nodemailer": "^6.9.14",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.1.1",
    "sharp": "^0.34.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.14.9",
    "@types/pg": "^8.15.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.5.4",
    "postcss": "^8.4.38",
    "prisma": "^7.0.1",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.20.5",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": ">=18.18.0 <23"
  }
}
